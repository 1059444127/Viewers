table#tblStudyList
  color: #888888
  border-collapse: collapse
  border-color: gray

  thead
    background-color: black
    white-space: nowrap
    
    &:hover
      color: #888888
      
  tbody > tr
    padding: 4px
    border: 1px solid #282828
    background-color: black

    td
      white-space:nowrap

    &:nth-of-type(odd)
      background-color: #202020

    &:hover
      background-color: #009BD2
      color: white


